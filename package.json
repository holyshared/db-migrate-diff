{
  "name": "db-migrate-diff",
  "version": "0.3.2",
  "description": "Migration difference detection module for db-migrate",
  "main": "lib/db-migrate-diff.js",
  "files": [
    "bin/db-migrate-diff",
    "lib/**/*.js",
    "README.md"
  ],
  "bin": {
    "db-migrate-diff": "./bin/db-migrate-diff"
  },
  "scripts": {
    "test": "multi='dot=- mocha-lcov-reporter=lcov-report.lcov' ./node_modules/.bin/mocha test",
    "build": "./node_modules/.bin/babel -d lib -s inline src",
    "watch": "./node_modules/.bin/babel -d lib -s inline -w src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holyshared/db-migrate-diff.git"
  },
  "author": "Noritaka Horio <holy.shared.design@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/db-migrate-diff/issues"
  },
  "homepage": "https://github.com/holyshared/db-migrate-diff",
  "dependencies": {
    "bluebird": "^3.0.5",
    "chalk": "^1.1.1",
    "cli-table": "^0.3.1",
    "lodash": "^4.0.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "blanket": "^1.2.1",
    "codecov.io": "^0.1.6",
    "coffee-script": "^1.10.0",
    "db-migrate": "^0.9.23",
    "eslint": "^1.10.3",
    "espower-coffee": "^1.0.0",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "mocha-multi": "^0.7.2",
    "power-assert": "^1.2.0"
  },
  "config": {
    "blanket": {
      "pattern": "/lib/",
      "data-cover-never": "/node_modules/"
    }
  }
}
